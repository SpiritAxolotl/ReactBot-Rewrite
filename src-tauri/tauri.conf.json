{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "reactbot-rewrite",
  "version": "0.1.0",
  "identifier": "zone.spax.reactbot",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "reactbot-rewrite",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": "default-src 'self'; img-src 'self' asset: http://asset.localhost; media-src 'self' asset: http://asset.localhost; script-src 'self';",
      "assetProtocol": {
        "enable": true,
        "scope": ["$APPLOCALDATA/GrabBag/**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
